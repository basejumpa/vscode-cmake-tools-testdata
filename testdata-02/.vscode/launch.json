{
    "configurations": [
        
        {
            "name": "test_some_unit / some_unit.some_function",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\build\\some_unit\\test_some_unit.exe",
            "args": [
                "--gtest_filter=some_unit.some_function"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}\\build\\Testing",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        },

        {
            "name": "test_some_unit / some_unit.some_other_function",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\build\\some_unit\\test_some_unit.exe",
            "args": [
                "--gtest_filter=some_unit.some_other_function"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}\\build\\Testing",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        },
    ],
    "version": "2.0.0"
}